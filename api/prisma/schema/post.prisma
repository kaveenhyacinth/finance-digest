model Post {
  id        Int      @id @default(sequence())
  title     String
  url       String
  image     String?
  provider  String   @default("blott")
  user      User?    @relation(fields: [userId], references: [id], onDelete: SetNull)
  userId    Int?
  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("posts")
}
